{
  "name": "work-document-blogs-frontend",
  "version": "1.0.0",
  "description": "存放工作室文档和博客",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "docs:clean": "vuepress dev docs --clean-cache",
    "docs:dev": "node --max-old-space-size=16384 node_modules/.bin/vuepress dev docs",
    "docs:build": "node --max-old-space-size=16384 node_modules/.bin/vuepress build docs",
    "docs:build-docker": "bash dockerfile_build.sh",
    "docs:update-package": "npx vp-update",
    "docs:update-git": "git pull",
    "docs:remote-git": "git add --all && git commit -m '自动热部署' && git push --force",
    "docs:run": "npm run docs:build && npm run && docs:remote"
  },
  "devDependencies": {
    "@vuepress-reco/tailwindcss-config": "^2.0.0-beta.23",
    "@vuepress/bundler-vite": "^2.0.0-rc.18",
    "@vuepress/plugin-append-date": "^2.0.0-rc.61",
    "@vuepress/plugin-catalog": "^2.0.0-rc.61",
    "@vuepress/plugin-comment": "^2.0.0-rc.61",
    "@vuepress/plugin-copyright": "^2.0.0-rc.61",
    "@vuepress/plugin-docsearch": "^2.0.0-rc.61",
    "@vuepress/plugin-feed": "^2.0.0-rc.61",
    "@vuepress/plugin-git": "^2.0.0-rc.60",
    "@vuepress/plugin-links-check": "^2.0.0-rc.65",
    "@vuepress/plugin-markdown-hint": "^2.0.0-rc.61",
    "@vuepress/plugin-markdown-image": "^2.0.0-rc.61",
    "@vuepress/plugin-markdown-math": "^2.0.0-rc.61",
    "@vuepress/plugin-markdown-stylize": "^2.0.0-rc.61",
    "@vuepress/plugin-markdown-tab": "^2.0.0-rc.61",
    "@vuepress/plugin-notice": "^2.0.0-rc.61",
    "@vuepress/plugin-photo-swipe": "^2.0.0-rc.61",
    "@vuepress/plugin-prismjs": "^2.0.0-rc.65",
    "@vuepress/plugin-reading-time": "^2.0.0-rc.61",
    "@vuepress/plugin-revealjs": "^2.0.0-rc.61",
    "@vuepress/plugin-search": "^2.0.0-rc.61",
    "@vuepress/plugin-shiki": "^2.0.0-rc.61",
    "@vuepress/plugin-watermark": "^2.0.0-rc.61",
    "@vuepress/theme-default": "^2.0.0-rc.11",
    "echarts": "^5.5.1",
    "katex": "^0.16.11",
    "mermaid": "^11.4.0",
    "sass-embedded": "^1.81.0",
    "vue": "^3.4.0",
    "vuepress": "^2.0.0-rc.18",
    "vuepress-plugin-markdown-define2": "^1.0.0",
    "vuepress-plugin-md-enhance": "^2.0.0-rc.59",
    "vuepress-theme-plume": "^1.0.0-rc.118",
    "vuepress-theme-reco": "^2.0.0-rc.25"
  },
  "dependencies": {
    "@bytemd/vue-next": "^1.21.0",
    "@element-plus/icons-vue": "^2.3.1",
    "@vuepress/client": "^2.0.0-rc.18",
    "element-plus": "^2.9.1",
    "md-editor-v3": "^5.1.1",
    "prettier": "^3.3.3",
    "vue-router": "^4.5.0",
    "vuepress-plugin-oh-my-live2d": "^0.19.3",
    "vuepress-plugin-remove-html-extension": "^1.15.0"
  }
}
