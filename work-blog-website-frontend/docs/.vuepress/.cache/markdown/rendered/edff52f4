line\"><span style=\"--shiki-light:#A0ADA0;--shiki-dark:#758575DD\">        //</span><span style=\"--shiki-light:#A0ADA0;--shiki-dark:#758575DD\">    //1.传统写法</span></span>\n<span class=\"line\"><span style=\"--shiki-light:#A0ADA0;--shiki-dark:#758575DD\">        //</span><span style=\"--shiki-light:#A0ADA0;--shiki-dark:#758575DD\">    //if (this != &#x26;str)</span></span>\n<span class=\"line\"><span style=\"--shiki-light:#A0ADA0;--shiki-dark:#758575DD\">        //</span><span style=\"--shiki-light:#A0ADA0;--shiki-dark:#758575DD\">    //{</span></span>\n<span class=\"line\"><span style=\"--shiki-light:#A0ADA0;--shiki-dark:#758575DD\">        //</span><span style=\"--shiki-light:#A0ADA0;--shiki-dark:#758575DD\">    //    std:: cout &#x3C;&#x3C; \" string operator =(const string&#x26; str)\" &#x3C;&#x3C; std:: endl;</span></span>\n<span class=\"line\"><span style=\"--shiki-light:#A0ADA0;--shiki-dark:#758575DD\">        //</span><span style=\"--shiki-light:#A0ADA0;--shiki-dark:#758575DD\">    //    char* cache = new char [str._capacity + 1];</span></span>\n<span class=\"line\"><span style=\"--shiki-light:#A0ADA0;--shiki-dark:#758575DD\">        //</span><span style=\"--shiki-light:#A0ADA0;--shiki-dark:#758575DD\">    //    strcpy(cache, str._s);</span></span>\n<span class=\"line\"><span style=\"--shiki-light:#A0ADA0;--shiki-dark:#758575DD\">        //</span><span style=\"--shiki-light:#A0ADA0;--shiki-dark:#758575DD\">    //    delete [] _s;</span></span>\n<span class=\"line\"><span style=\"--shiki-light:#A0ADA0;--shiki-dark:#758575DD\">        //</span><span style=\"--shiki-light:#A0ADA0;--shiki-dark:#758575DD\">    //    _s = cache;</span></span>\n<span class=\"line\"><span style=\"--shiki-light:#A0ADA0;--shiki-dark:#758575DD\">        //</span><span style=\"--shiki-light:#A0ADA0;--shiki-dark:#758575DD\">    //    _size = str._size;</span></span>\n<span class=\"line\"><span style=\"--shiki-light:#A0ADA0;--shiki-dark:#758575DD\">        //</span><span style=\"--shiki-