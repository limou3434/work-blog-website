 return this.tableData.slice(start, end); // è·å–å½“å‰é¡µçš„æ•°æ®\n    }\n  },\n  methods: {\n    handleOpen(key, keyPath) {\n      console.log(key, keyPath); // å½“å±•å¼€æ—¶, è¿™é‡Œä¼šè¾“å‡ºæ§åˆ¶å°çš„å›è°ƒä¿¡æ¯\n    },\n    handleClose(key, keyPath) {\n      console.log(key, keyPath); // å½“å…³é—­æ—¶, è¿™é‡Œä¼šè¾“å‡ºæ§åˆ¶å°çš„å›è°ƒä¿¡æ¯\n    },\n    handleSelect(key) { // å½“é€‰ä¸­æŸä¸ªèœå•é¡¹æ—¶, è¿™é‡Œä¼šä¿®æ”¹æ•°æ®æ¨¡å‹ä¸­çš„æ•°æ®\n      this.activeMenu = key;\n    },\n    handlePageChange(page) { // å½“é¡µé¢å‘ç”Ÿæ”¹å˜æ—¶è§¦å‘\n      this.currentPage = page; // æ•°æ®æ¨¡å‹ä¸­å­˜å‚¨ä¸€ä»½é¡µé¢æ•°æ®, å¯¼è‡´åŒæ­¥è¢«è§¦å‘, ä½¿å¾—ä¹‹å‰ç¼“å­˜çš„è®¡ç®—å±æ€§ currentPageData() è¢«é‡æ–°è¿›è¡Œè®¡ç®—\n    },\n    handleSizeChange(size) { // å½“ç”¨æˆ·é€‰æ‹©ä¸åŒçš„æœ€å¤§æ˜¾ç¤ºæ¡æ•°æ—¶è§¦å‘\n      this.pageSize = size; // æ•°æ®æ¨¡å‹ä¸­å­˜å‚¨ä¸€ä»½é¡µé¢æ•°æ®, å¯¼è‡´åŒæ­¥è¢«è§¦å‘, ä½¿å¾—ä¹‹å‰ç¼“å­˜çš„è®¡ç®—å±æ€§ currentPageData() è¢«é‡æ–°è¿›è¡Œè®¡ç®—\n      this.currentPage = 1; // å¹¶ä¸”åˆ‡æ¢æ¯é¡µæ¡æ•°åé»˜è®¤é‡ç½®ä¸ºç¬¬ä¸€é¡µ\n      this.paginationKey += 1; // æ”¹å˜ key ä»¥é‡æ–°æ¸²æŸ“åˆ†é¡µç»„ä»¶\n    }\n  }\n}\n</script>\n\n<style scoped>\n/* ä½ çš„æ ·å¼å¯ä»¥å†™åœ¨è¿™é‡Œ */\n</style>\n\n```\n\n```js\n<!-- NavMenuItem/NavContentTwo.vue -->\n<template>\n  <div>\n    <h2>å†…å®¹äºŒ</h2>\n    <p>è¿™æ˜¯å¯¼èˆªäºŒçš„æ­£æ–‡å†…å®¹...</p>\n    <!-- æ·»åŠ æ›´å¤šå†…å®¹ -->\n  </div>\n</template>\n\n<script>\nexport default {\n  name: 'NavContentOne', // ç”±äºåšæˆäº†ç»„ä»¶, è¿™é‡Œå°±å¿…é¡»å¯¼å‡ºç»„ä»¶çš„åå­—äº†!\n}\n</script>\n\n<style>\n/* æ·»åŠ å¿…è¦çš„æ ·å¼ */\n</style>\n\n```\n\nè‡³äºåé¢çš„ `NavMenuItem/NavContentThree.vue, NavMenuItem/NavContentFour.vue, NavMenuItem/NavContentFive.vue, NavMenuItem/NavContentSix.vue, NavMenuItem/NavContentSeven.vue, NavMenuItem/NavContentEight.vue, NavMenuItem/NavContentNine.vue` å’Œ `NavMenuItem/NavContentTwo.vue` çš„å†…å®¹å·®ä¸å¤šï¼Œæ‚¨çœ‹è¿™å†™å°±è¡Œã€‚\n\n## 4.2.ç½‘ç»œç»„ä»¶\n\n### 4.2.1.å¼•å…¥ç»„ä»¶\n\nåœ¨ `vue-cli` ç”Ÿæˆçš„ `Vue2.js` å·¥ç¨‹ä¸­ï¼Œè™½ç„¶æ‚¨ä¹Ÿå¯ä»¥é€‰æ‹©ç›´æ¥æŠŠ `ajax.js` ç›´æ¥å¯¼å…¥æ¥ä½¿ç”¨ï¼Œä½†æ˜¯æˆ‘ä»¬ä¸€èˆ¬ä¸æ¨èè¿™ä¹ˆåšï¼Œè€Œæ˜¯é€šè¿‡ `npm install axios` çš„æ–¹å¼ä¸‹è½½ï¼Œå†åœ¨ `main.js` ä¸­ä½¿ç”¨ `import axios from 'axios';` è¿›è¡Œå¯¼å…¥ã€‚\n\n### 4.2.2.ä½¿ç”¨ç»„ä»¶\n\nç”±äº `axios-ajax` åªè¦ç†Ÿæ‚‰ `HTTP` æ‰‹å†Œçš„å¼€å‘ï¼Œå³å¯è¿›è¡Œå¿«é€Ÿç¼–å†™ï¼Œæˆ‘è¿™é‡Œå°±å»ºç«‹ä¸€ä¸ªç®€å•çš„ `Node.js` ç¨‹åºï¼Œåªéœ€è¦åœ¨è§†å›¾ä¸­ç®€å•è¿›è¡Œè¯·æ±‚è¿”å›æ•°æ®å³å¯ï¼Œæ›´å¤šå¤æ‚çš„éœ€æ±‚åé¢ç¼–å†™ä¸€ä¸ªå°å·¥ç¨‹çš„æ—¶å€™å†ç»™æ‚¨è¯¦ç»†è§£é‡Šã€‚\n\n>   åæ§½ï¼šå¦‚æœæ‚¨å®Œå…¨æ²¡æœ‰å­¦è¿‡ `HTTP` åè®®ï¼Œæˆ‘å»ºè®®æ‚¨å…ˆå»äº†è§£ä¸ªå¤§æ¦‚å­...å¦åˆ™å°±å…ˆæ­¢æ­¥äºè¿™é‡Œå…ˆå­ï¼ä¸è¦ç»§ç»­å¾€åçœ‹ä¸‹å»äº†ï¼Œè™½ç„¶ä¸éš¾ä½†æ²¡æœ‰è¶³å¤ŸçŸ¥è¯†å‚¨å¤‡æ˜¯æ²¡åŠæ³•çœ‹æ‡‚çš„ï¼Œæ‰€ä»¥è¯·ä¸è¦ç°å¿ƒï¼Œâ€œä»¥åå†æ¥æ¢ç´¢å­~â€...\n\né¦–å…ˆæˆ‘ä»¬å¿…é¡»æœ‰ä¸€ä¸ªæœåŠ¡ç«¯ç¨‹åºï¼Œæœ€å¥½æ˜¯åŸºäº `HTTP` åè®®çš„ï¼Œè¿™é‡Œæˆ‘å·²ç»ä½¿ç”¨ `Node.js` ç¼–å†™å¥½äº†ï¼Œåªè¦æ‚¨ä¹‹å‰æœ‰å®‰è£…è¿‡ `Node.js` å°±å¯ä»¥ç›´æ¥ä½¿ç”¨ `node http_web_server.js` è¿è¡Œèµ·æ¥ã€‚\n\n```js\n// http_web_server.js\nconst express = require('express');\nconst app = express();\nconst PORT = 8083;\n\napp.get('/', (req, res) => {\n    res.send('Hello, World!');\n});\n\napp.listen(PORT, '127.0.0.1', () => {\n    console.log(`Server is running on http://127.0.0.1:${PORT}`);\n});\n\n```\n\næˆ‘ä»¬å…ˆè¿è¡Œèµ·æ¥è¿™ä¸ªæœåŠ¡ç«¯ç¨‹åºã€‚\n\n```shell\n# è¿è¡Œ Node.js ç¼–å†™çš„æœåŠ¡ç«¯ç¨‹åº\n$ node http_web_server.js\nServer is running on http://127.0.0.1:port # è¿™é‡Œçš„ port æ ¹æ®æ‚¨çš„è®¾ç½®è€Œå®š\n```\n\nç„¶åç¼–å†™å¯¹åº”çš„è§†å›¾å‡½æ•°ã€‚\n\n```js\n<!-- EllementAjaxView.vue -->\n<template>\n  <div>\n    <h1>æµ‹è¯• Express æœåŠ¡å™¨</h1>\n    <button @click=\"fetchData\">è·å–æ•°æ®</button>\n    <div v-if=\"loading\">åŠ è½½ä¸­...</div>\n    <div v-if=\"error\" class=\"error\">é”™è¯¯: {{ error }}</div>\n    <div v-if=\"data\">\n      <h2>è¿”å›çš„æ•°æ®:</h2>\n      <pre>{{ data }}</pre>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from 'axios';\n\nexport default {\n  data() {\n    return {\n      data: null,\n      error: null,\n      loading: false,\n    };\n  },\n  methods: {\n    async fetchData() {\n      this.loading = true;\n      this.error = null;\n      try {\n        const response = await axios.get('http://127.0.0.1:port/'); // è¿™é‡Œçš„ port ä¹Ÿéœ€è¦æ‚¨æ‰‹åŠ¨ä¿®æ”¹\n        this.data = response.data;\n      } catch (err) {\n        this.error = err.message;\n      } finally {\n        this.loading = false;\n      }\n    },\n  },\n};\n</script>\n\n<style scoped>\n.error {\n  color: red;\n}\n</style>\n\n```\n\n```js\n<!-- App.vue -->\n<template>\n  <div id=\"app\">\n    <nav> <!-- è¿™ä¸ª nav åªæ˜¯ä¸€ä¸ªè¯­ä¹‰æ ‡ç­¾, ä¸»è¦æ˜¯ç”¨åœ¨é¡µé¢ä¸­çš„å¯¼èˆªé“¾æ¥éƒ¨åˆ†, å®ƒé€šå¸¸åŒ…å«ä¸€ç»„é“¾æ¥, ç”¨æˆ·å¯ä»¥é€šè¿‡è¿™äº›é“¾æ¥åœ¨ç½‘ç«™æˆ–åº”ç”¨çš„ä¸åŒéƒ¨åˆ†ä¹‹é—´è¿›è¡Œå¯¼èˆª, ä¸»è¦æ˜¯ä¸ºäº†å¢åŠ ä»£ç å¯è¯»æ€§å’Œæœç´¢å¼•æ“ SEO å‹å¥½çš„ -->\n      <router-link to=\"/\">Home</router-link> <!-- æ˜¾ç¤º Home è§†å›¾é“¾æ¥ -->\n      |\n      <router-link to=\"/about\">about</router-link> <!-- æ˜¾ç¤º about è§†å›¾é“¾æ¥ -->\n      |\n      <router-link to=\"/button\">button</router-link> <!-- æ˜¾ç¤º about è§†å›¾é“¾æ¥ -->\n      |\n      <router-link to=\"/table\">table</router-link> <!-- æ˜¾ç¤º about è§†å›¾é“¾æ¥ -->\n      |\n      <router-link to=\"/pagination\">pagination</router-link> <!-- æ˜¾ç¤º about è§†å›¾é“¾æ¥ -->\n      |\n      <router-link to=\"/dialog\">dialog</router-link> <!-- æ˜¾ç¤º about è§†å›¾é“¾æ¥ -->\n      |\n      <router-link to=\"/from\">from</router-link> <!-- æ˜¾ç¤º about è§†å›¾é“¾æ¥ -->\n      |\n      <router-link to=\"/container\">container</router-link> <!-- æ˜¾ç¤º about è§†å›¾é“¾æ¥ -->\n      |\n      <router-link to=\"/navMenu\">navMenu</router-link> <!-- æ˜¾ç¤º about è§†å›¾é“¾æ¥ -->\n      |\n      <router-link to=\"/ajax\">ajax</router-link> <!-- æ˜¾ç¤º about è§†å›¾é“¾æ¥ -->\n    </nav>\n    <router-view/> <!-- é¡µé¢/è§†å›¾çš„æ˜¾ç¤ºåœ°ç‚¹ -->\n  </div>\n</template>\n\n<style>\n#app {\n  font-family: Avenir, Helvetica, Arial, sans-serif;\n  -webkit-font-smoothing: antialiased;\n  -moz-osx-font-smoothing: grayscale;\n  text-align: center;\n  color: #2c3e50;\n}\n\nnav {\n  padding: 30px;\n}\n\nnav a {\n  font-weight: bold;\n  color: #2c3e50;\n}\n\nnav a.router-link-exact-active {\n  color: #42b983;\n}\n</style>\n<script setup lang=\"ts\">\n</script>\n```\n\n```js\n// index.js\nimport Vue from 'vue'\nimport VueRouter from 'vue-router'\nimport HomeView from '../views/HomeView.vue'\nimport ButtonView from '../views/ElementButtonView.vue'\nimport TableView from '../views/ElementTableView.vue'\nimport PaginationView from '../views/ElementPaginationView.vue'\nimport DialogView from '../views/ElementDialogView.vue' // æ³¨æ„è¿™é‡Œè¦å¯¼å…¥\nimport FromView from '../views/ElementFormView.vue' // æ³¨æ„è¿™é‡Œè¦å¯¼å…¥\nimport Container from '../views/ElementContainerView.vue' // æ³¨æ„è¿™é‡Œè¦å¯¼å…¥\nimport NavMenu from '../views/ElementNavMenuView.vue' // æ³¨æ„è¿™é‡Œè¦å¯¼å…¥\nimport Ajax from '../views/ElementAjaxView.vue' // æ³¨æ„è¿™é‡Œè¦å¯¼å…¥\n\nVue.use(VueRouter)\n\n// å®šä¹‰è·¯ç”±æ˜ å°„\nconst routes = [\n    {\n        path: '/',\n        name: 'home', // è¿™é‡Œçš„ name æ˜¯è·¯ç”±åå­—, è·¯ç”±åå­—å¯ä»¥è®©è·¯ç”±ä¿®æ”¹çš„æ—¶å€™ä¸æ˜¯ç¡¬ç¼–ç è®¾ç½®, æ–¹ä¾¿ä¸šåŠ¡éšæ—¶è¿›è¡Œä¿®æ”¹\n        component: HomeView // æ³¨å†Œå­ç»„ä»¶, ä¹Ÿå°±æ˜¯è§†å›¾\n    },\n    {\n        path: '/about',\n        name: 'about',\n        // ä¸‹é¢è¿™ç§åŠ¨æ€åŠ è½½å®é™…ä¸Šæ˜¯æ‡’åŠ è½½, åªæœ‰ç”¨æˆ·å®é™…è®¿é—®äº† /about è·¯å¾„æ—¶, æ‰ä¼šè¿›è¡Œä¸‹è½½å’Œæ¸²æŸ“, å¯ä»¥æé«˜ç”¨æˆ·çš„å“åº”é€Ÿåº¦\n        component: () => import(/* webpackChunkName: \"about\" */ '../views/AboutView.vue') // è¿™ä¹Ÿæ˜¯ä¸ºä»€ä¹ˆä¹‹å‰æˆ‘ä»¬è®¾ç½®çš„ about æ²¡æœ‰è®¾ç½® name çš„åŸå› \n    },\n    { // ä¸‹é¢æ˜¯æ–°å¢åŠ çš„è·¯ç”±\n        path: '/button',\n        name: 'button', // è¿™é‡Œçš„ name æ˜¯è·¯ç”±åå­—, è·¯ç”±åå­—å¯ä»¥è®©è·¯ç”±ä¿®æ”¹çš„æ—¶å€™ä¸æ˜¯ç¡¬ç¼–ç è®¾ç½®, æ–¹ä¾¿ä¸šåŠ¡éšæ—¶è¿›è¡Œä¿®æ”¹\n        component: ButtonView // æ³¨å†Œå­ç»„ä»¶, ä¹Ÿå°±æ˜¯è§†å›¾\n 